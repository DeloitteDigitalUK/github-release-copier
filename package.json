  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.19",
    "@types/node-fetch": "^2.6.11",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3"
  }
}
